<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_inject"
        android:text="inject 无参"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn_inject_2"
        android:text="inject 有参"
        android:layout_toEndOf="@id/btn_inject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn_binds"
        android:text="binds注入接口"
        android:layout_below="@id/btn_inject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</RelativeLayout>